msgid "Brings smart typographic replacements for your blog entries and comments"
msgstr "Applique des remplacements typographiques pour les billets et les commentaires de votre blog"

msgid "Typo"
msgstr "Typo"

msgid "Typographic replacements"
msgstr "Remplacements typographiques"

msgid "Warning! These replacements will not be undoable."
msgstr "Attention ces remplacements sont irréversibles."

msgid "save"
msgstr "enregistrer"

msgid "\"--\" for em-dashes; no en-dash support (default)"
msgstr "\"--\" (2) pour les tirets longs (em-dashes) ; pas de support des tirets courts (en-dash, option par défaut)"

msgid "\"---\" for em-dashes; \"--\" for en-dashes"
msgstr "\"---\" (3) pour les tirets longs (em-dashes) ; \"--\" (2) pour les tirets courts (en-dash)"

msgid "\"--\" for em-dashes; \"---\" for en-dashes"
msgstr "\"--\" (2) pour les tirets longs (em-dashes) ; \"---\" (3) pour les tirets courts (en-dash)"

msgid "Enable typographic replacements for this blog"
msgstr "Activer les remplacements typographiques pour ce blog"

msgid "Enable typographic replacements for entries"
msgstr "Activer les remplacements typographiques pour les billets de ce blog"

msgid "Enable typographic replacements for comments"
msgstr "Activer les remplacements typographiques pour les commentaires de ce blog"

msgid "Excluding trackbacks"
msgstr "Rétroliens exclus"

msgid "Dashes replacement mode"
msgstr "Mode de remplacement des tirets multiples"
